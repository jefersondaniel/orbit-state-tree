{
  "name": "orbit-state-tree",
  "version": "1.0.0",
  "description": "State tree manager based on Orbit.js",
  "main": "dist/orbit-state-tree.js",
  "module": "dist/orbit-state-tree.mjs",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "lint": "standard",
    "test": "mocha --require babel-core/register test/*.js",
    "prepublish": "npm run build && npm test"
  },
  "keywords": [
    "orbit.js",
    "flux",
    "state-tree",
    "json-api"
  ],
  "author": "Jeferson Daniel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jefersondaniel/orbit-state-tree.git"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist"
    ]
  },
  "devDependencies": {
    "@orbit/data": "^0.16.3",
    "@orbit/memory": "^0.16.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-watch": "^4.3.1",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "@orbit/core": "^0.16.3",
    "invariant": "^2.2.4",
    "json-api-normalizer": "^0.4.16",
    "jsona": "^1.5.0",
    "redux-object": "^0.5.10"
  }
}
