{
  "name": "orbit-state-tree",
  "version": "0.1.5",
  "description": "State tree manager based on Orbit.js",
  "main": "dist/orbit-state-tree.js",
  "module": "dist/orbit-state-tree.mjs",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "lint": "standard",
    "test": "mocha --compilers js:babel-register test/*.js",
    "prepublish": "npm run build && npm test"
  },
  "keywords": [
    "orbit.js",
    "flux",
    "state-tree",
    "json-api"
  ],
  "author": "Jeferson Daniel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jefersondaniel/orbit-state-tree.git"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist"
    ]
  },
  "devDependencies": {
    "@orbit/data": "^0.15.6",
    "@orbit/store": "^0.15.6",
    "babel-eslint": "^8.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-watch": "^4.3.1",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "@orbit/core": "^0.15.7",
    "invariant": "^2.2.2",
    "json-api-normalizer": "^0.4.1",
    "jsona": "^1.0.12",
    "redux-object": "^0.5.1"
  }
